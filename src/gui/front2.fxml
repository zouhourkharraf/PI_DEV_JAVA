    <ImageView fx:id="imageView" fitWidth="200" fitHeight="200" GridPane.columnIndex="0" GridPane.rowIndex="0" GridPane.rowSpan="3" />
    <Label fx:id="labelNom" styleClass="title" GridPane.columnIndex="1" GridPane.rowIndex="0" />
    <Label fx:id="labelLieu" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <Label fx:id="labelDate" GridPane.columnIndex="1" GridPane.rowIndex="2" />
</GridPane>






