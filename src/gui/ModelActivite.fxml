<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="60.0" prefWidth="866.4" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Model.ActiviteModelController">
   <children>
      <Label fx:id="nomLabel" alignment="CENTER" prefHeight="27.0" prefWidth="141.0" text="Nom ...">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <HBox.margin>
            <Insets top="10.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="positionLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="27.0" prefWidth="114.0" text="Position">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
         <HBox.margin>
            <Insets top="10.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="dateLabel" alignment="CENTER" prefHeight="20.0" prefWidth="91.0" text="Date">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
         <HBox.margin>
            <Insets top="10.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="nbLabel" alignment="CENTER" prefHeight="27.0" prefWidth="192.0" text="Nombre participants">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
         <HBox.margin>
            <Insets top="10.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="typeLabel" alignment="CENTER" prefHeight="20.0" prefWidth="66.0" text="Type">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
         <HBox.margin>
            <Insets top="10.0" />
         </HBox.margin>
      </Label>
      <HBox prefHeight="60.0" prefWidth="120.0">
         <children>
            <Button fx:id="descButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="39.0" prefWidth="105.0" text="Voir description">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox prefHeight="60.0" prefWidth="106.0">
         <children>
            <Button fx:id="participerButton" mnemonicParsing="false" prefHeight="39.0" prefWidth="105.0" text="Participer">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</HBox>
